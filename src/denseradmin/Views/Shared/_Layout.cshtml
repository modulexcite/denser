<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=9" >
    <title>Denser Management</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ace/ace.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ace/theme-clouds.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/ace/mode-javascript.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <div id="header" width="100%">
            <table>
                <tr>
                    <td>
                        <img src="@Url.Content("~/Content/shaun.png")" />
                    </td>
                    <td>
                        <h1>
                            Denser&nbsp;Management</h1>
                    </td>
                    <td width="100%">
                        <div id="logindisplay">
                            @Html.Partial("_LogOnPartial")
                        </div>
                        <div id="menucontainer">
                            <ul id="menu">
                                <li>@Html.ActionLink("Programs", "Programs", "Denser")</li>
                                <li>@Html.ActionLink("New program", "NewProgram", "Denser")</li>
                                <li>@Html.ActionLink("About", "About", "Denser")</li>
                            </ul>
                        </div>
                    </td>
                </tr>
            </table>
        </div>
        <div id="main">
            @RenderBody()
            <div id="footer">
            </div>
        </div>
    </div>
</body>
</html>
